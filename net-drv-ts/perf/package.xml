                        <value>0</value>
                        <value>0</value>
                    </arg>
                </run>

                <run name="tcp_perf3_rx_ring" template="tcp_udp_perf">
                    <script name="tcp_udp_perf">
                        <objective>Measure TCP performace using iperf3 depending on Rx ring size</objective>
                    </script>
