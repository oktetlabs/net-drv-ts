                <run name="tcp_udp_perf_fwd">
                    <session>

                        <var name="env.peer2peerX2" global="true">
                            <value>
                                'net0':IUT{
                                    'iut_host'{
                                        if:'iut_if0',
                                        addr:'iut4_addr0':inet:unicast,
                                        addr:'iut6_addr0':inet6:unicast
                                    },
                                    'tst_host'{
                                        if:'tst_if0',
                                        addr:'tst4_addr0':inet:unicast,
                                        addr:'tst6_addr0':inet6:unicast
                                    }
                                },
                                'net1':IUT{
                                    'iut_host'{
                                        if:'iut_if1',
                                        addr:'iut4_addr1':inet:unicast,
                                        addr:'iut6_addr1':inet6:unicast
                                    },
                                    'tst_host'{
