                    </arg>
                    <arg name="dual_mode" type="boolean"/>
                    <arg name="protocol">
                        <value>IPPROTO_UDP</value>
                    </arg>
                    <arg name="n_streams">
                        <value>1</value>
                        <value>2</value>
                        <value>4</value>
                        <value>8</value>
                        <value>16</value>
                    </arg>
                    <arg name="bandwidth">
                        <value>1000</value>
                    </arg>
                </run>

                <run name="udp_perf3" template="tcp_udp_perf">
