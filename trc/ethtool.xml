<test name="ethtool" type="package">
  <objective>Ethtool tests</objective>
  <notes/>
  <iter result="PASSED">
    <notes/>
    <test name="dev_properties" type="script">
      <objective>Check that ethtool shows interface properties.</objective>
      <notes/>
      <iter result="PASSED">
        <arg name="env"/>
        <notes/>
      </iter>
    </test>
    <test name="statistics" type="script">
      <objective>Check that ethtool shows interface statistics.</objective>
      <notes/>
      <iter result="PASSED">
        <arg name="env"/>
        <arg name="sock_type"/>
        <notes/>
      </iter>
    </test>
    <test name="msglvl" type="script">
      <objective>Check that changing driver message level affects logs printed by the driver.</objective>
      <notes/>
      <iter result="PASSED">
        <arg name="env"/>
        <notes/>
        <results tags="ice" key="WONTFIX ETHTOOL-MSGLVL" notes="ice does not have logs enabled via msglvl">
          <result value="FAILED">
            <verdict>No logs from the driver were detected when all flags in msglvl are switched on</verdict>
          </result>
        </results>
        <results tags="igb" key="WONTFIX IGB-ETHTOOL-MSGLVL" notes="igb does not have logs enabled via msglvl">
          <result value="FAILED">
            <verdict>No logs from the driver were detected when all flags in msglvl are switched on</verdict>
          </result>
        </results>
        <results tags="virtio-pci" key="WONTFIX VIRTIO-ETHTOOL-MSGLEVEL" notes="VirtIO net does not support message level change via ethtool">
          <result value="FAILED">
            <verdict>Failed to enable all flags in msglvl</verdict>
          </result>
        </results>
      </iter>
    </test>
    <test name="reset_under_traffic" type="script">
      <objective>Check that NIC can be reset when data is transmitted over it and that data transmission resumes after NIC is reset.</objective>
      <notes/>
      <iter result="PASSED">
        <arg name="env"/>
        <arg name="flags">MAC</arg>
        <notes/>
        <results tags="i40e" key="WONTFIX ETHTOOL-RESET" notes="intel/i40e does not support reset via ethtool">
          <result value="SKIPPED">
            <verdict>ETHTOOL_RESET is not supported</verdict>
          </result>
        </results>
        <results tags="ice" key="WONTFIX ETHTOOL-RESET" notes="intel/ice does not support reset via ethtool">
          <result value="SKIPPED">
            <verdict>ETHTOOL_RESET is not supported</verdict>
